{
  "manifest_version": 2,

  "name": "HCI Project",
  "description": "Tab-less professor reviews within BroncoDirect",
  "version": "1.0",
  "content_scripts": [{
    "run_at": "document_end",
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": ["js/jquery-3.2.1.js", "js/sidepanel.js"],
    "css": ["css/style.css"]
  }],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://ec2-52-53-153-54.us-west-1.compute.amazonaws.com/",
    "https://0177ebc8.ngrok.io"
  ],
  "web_accessible_resources": ["popup.html","css/style.css"]
}
